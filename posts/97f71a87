            str = str.replace(/<a.*?>.*?<\/a>/g, '[é“¾æ¥]');
            str = str.replace(/<pre.*?>.*?<\/pre>/g, '[ä»£ç å—]');
            str = str.replace(/<.*?>/g, '');
            return str
          }
